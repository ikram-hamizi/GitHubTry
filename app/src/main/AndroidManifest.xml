<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usp05.githubtry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user_handling.Register" />
        <activity android:name=".item_manipulation.AddItemActivity" />
        <activity android:name=".inventory_display.InventoryActivity" />
        <activity android:name=".item_manipulation.ItemDisplayDetails" />
        <activity android:name=".item_manipulation.EditItemActivity" />
        <activity android:name=".item_filtering.FilterActivity" />
        <activity android:name=".user_handling.ChangePassword" />
        <activity android:name=".user_handling.ForgotPassword" />
        <receiver android:name=".notifications.NotificationPublisher"></receiver>
    </application>

</manifest>